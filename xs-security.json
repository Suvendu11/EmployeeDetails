{
  "xsappname": "empdetais",
  "tenant-mode": "dedicated",
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "uaa.user",
      "description": "UAA"
    },
    {
      "name": "$XSAPPNAME.Viewer",
      "description": "Viewer"
    },
    {
      "name": "$XSAPPNAME.Admin",
      "description": "Administrator"
    }
  ],
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA Token Exchange",
      "scope-references": [
        "uaa.user"
      ]
    },
    {
      "name": "Viewer",
      "description": "Viewer",
      "scope-references": [
        "$XSAPPNAME.Viewer"
      ]
    },
    {
      "name": "Admin",
      "description": "Administrator",
      "scope-references": [
        "$XSAPPNAME.Admin"
      ]
    }
  ]
}
