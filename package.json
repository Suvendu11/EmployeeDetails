{
    "name": "empdetais",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^7",
        "@sap/cds-hana": "^2",
        "@sap/xssec": "^3",
        "express": "^4",
        "@sap-cloud-sdk/http-client": "^2.9.0"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "@sap/ux-specification": "^1.120.6",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-employeeapp": "cds watch --open employeeapp/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy empdetais --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
    },
    "cds": {
        "requires": {
            "[production]": {
                "db": "hana",
                "auth": "mocked"
            },
            "spa_api": {
                "kind": "rest",
                "credentials":{
                    "destination": "YourDestinationName"
                }
            }
        }
    }
}